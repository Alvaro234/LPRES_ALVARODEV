<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<experiment id="OD_designpoint" version="1.3.0" component="CFM56_Turbofan" partition="Off_design">
 <header />
 <options>
  <maximumTimeCalculation id="maxTimeCalculation" maxTime="" active="no" />
 </options>
 <case id="OD_designpoint" description="Description" run="yes" noplotted="no" type="standard case" deploy="true">
  <restoreState id="ImportGeometry" description="Restore State">
   <general>
    <run value="yes" />
    <initialization value="yes" />
    <restore type="RESTORE_STATE" filename="@LPRES@/experiments/+c+f+m56_+turbofan.+design/+design/report.xml" format="T_TEXT" category="" filterEquiv="TRUE" />
   </general>
  </restoreState>
  <steady id="steady2" description="Steady">
   <general>
    <caltype />
    <nlsolver smethod="NR" tolerance="1e-06" toltype="RELTOL" maxiter="10000" maxfval="40000" maxjaceval="60" maxbroyden="60" rdigits="9" />
    <stopSimulation stopBadOperation="no" />
    <debug level="1" />
    <report id="report.xml" vars="*" type="SAVE_VARS" filterEquivalents="TRUE" alphasort="TRUE" create="yes" />
    <initialization value="no" />
    <run value="yes" />
   </general>
   <global>
    <bounds>
     <variable id="Inlet_1.Tt" alias="" value="1550" />
     <variable id="Inlet_1.pt" alias="" value="31*101325" />
     <variable id="LPRES.Altitude" alias="" value="0" />
     <variable id="Turbine_ch_1.rpm" alias="" value="12000" />
    </bounds>
    <initial>
     <variable id="Nozzle_1.g.pt" alias="" value="1" dxAbs="1.5e-08" dxRel="1.5e-08" maxStepAbs="1.79769313e+308" maxStepRel="1.79769313e+308" />
    </initial>
    <closure>
     <equation left="Nozzle_1.A_sl" oper="=" right="ZONE (Nozzle_1.M_out &gt; 0) Nozzle_1.A * Nozzle_1.M_out / ((2 + (LPRES.gamma(Inlet_1.fluidV) - 1) * Nozzle_1.M_out ** 2) / (LPRES.gamma(Inlet_1.fluidV) + 1)) ** ((LPRES.gamma(Inlet_1.fluidV) + 1) / (2 * (LPRES.gamma(Inlet_1.fluidV) - 1))) OTHERS Nozzle_1.A * -Nozzle_1.M_out / ((2 + (LPRES.gamma(Inlet_1.fluidV) - 1) * Nozzle_1.M_out ** 2) / (LPRES.gamma(Inlet_1.fluidV) + 1)) ** ((LPRES.gamma(Inlet_1.fluidV) + 1) / (2 * (LPRES.gamma(Inlet_1.fluidV) - 1)))" toltype="ABSTOL" tolerance="1e-06" internal="yes" />
    </closure>
   </global>
  </steady>
 </case>
</experiment>
